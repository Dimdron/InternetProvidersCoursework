         </children>
      </AnchorPane>

      <VBox prefHeight="63.0" prefWidth="208.0">
         <children>
            <AnchorPane prefHeight="27.0" prefWidth="208.0">
               <children>
                  <Label text="Speed" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
