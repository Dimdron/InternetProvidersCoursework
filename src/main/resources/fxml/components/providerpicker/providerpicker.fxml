<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root alignment="TOP_CENTER" prefWidth="250.0" spacing="15.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="26.0" prefWidth="282.0">
         <children>
            <Label text="Connection" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10" AnchorPane.topAnchor="10.0" />
            <ComboBox fx:id="cbConnection" editable="false" prefWidth="150.0" promptText="Any" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5" AnchorPane.topAnchor="5.0" />
         </children>
      </AnchorPane>

      <AnchorPane prefHeight="27.0" prefWidth="208.0">
         <children>
            <Label text="Region" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
