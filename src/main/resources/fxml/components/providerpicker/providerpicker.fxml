                  <Label layoutX="80.0" layoutY="10.0" text="from:" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="80.0" AnchorPane.topAnchor="10.0" />
                  <TextField fx:id="tfSpeed" layoutX="120.0" layoutY="2.0" prefHeight="22.0" prefWidth="47.0" AnchorPane.bottomAnchor="8.0" AnchorPane.rightAnchor="73.0" AnchorPane.topAnchor="2.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>

      <VBox prefHeight="64.0" prefWidth="208.0">
         <children>
            <AnchorPane prefHeight="23.0" prefWidth="208.0">
               <children>
                  <Label text="Price" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
                   <Slider fx:id="sldPrice" prefHeight="9.0" prefWidth="140.0" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0" />
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="100.0" prefWidth="200.0">
