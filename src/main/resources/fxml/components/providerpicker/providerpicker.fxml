                  <Label layoutX="77.0" layoutY="10.0" text="up to:" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="77.0" AnchorPane.topAnchor="10.0" />
                  <TextField fx:id="tfPrice" layoutX="120.0" layoutY="5.0" prefHeight="24.0" prefWidth="47.0" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="73.0" AnchorPane.topAnchor="5.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
      <AnchorPane>
          <children>
            <Button fx:id="btnFind" mnemonicParsing="false" text="Find" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="10.0" />
